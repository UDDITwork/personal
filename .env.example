# LlamaCloud API Key (get from https://cloud.llamaindex.ai)
LLAMA_CLOUD_API_KEY=llx-xxxxxxxxxxxxx

# Google Gemini API Key (get from https://aistudio.google.com/apikey)
GEMINI_API_KEY=xxxxxxxxxxxxxxx

# Redis URL for Celery broker
REDIS_URL=redis://localhost:6379/0

# Concurrency Settings
MAX_CONCURRENT_EXTRACTIONS=50

# Processing Timeouts (seconds)
EXTRACTION_TIMEOUT=300
DIAGRAM_DESCRIPTION_TIMEOUT=60

# Environment
ENVIRONMENT=development
